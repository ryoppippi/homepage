{
  "name": "homepage",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.21.0",
  "description": "TechLead Inc. corporate homepage",
  "scripts": {
    "dev": "pnpm -r --parallel --stream dev",
    "build": "pnpm --filter worker build && pnpm --filter web build",
    "build:web": "pnpm --filter web build",
    "build:worker": "pnpm --filter worker build",
    "preview": "pnpm --filter web preview",
    "typecheck": "pnpm -r run typecheck",
    "typecheck:web": "pnpm --filter web typecheck",
    "typecheck:worker": "pnpm --filter worker typecheck",
    "format": "pnpm -r run format",
    "format:check": "pnpm -r run format:check",
    "lint": "pnpm -r run lint",
    "lint:fix": "pnpm -r run lint:fix",
    "check": "pnpm -r run check",
    "check:web": "pnpm --filter web check",
    "check:worker": "pnpm --filter worker check",
    "check:shared": "pnpm --filter @homepage/shared check",
    "check:ci": "pnpm -r run check:ci",
    "deploy:worker": "pnpm --filter worker deploy"
  },
  "devEngines": {
    "runtime": [
      {
        "name": "node",
        "version": "^24.2.0",
        "onFail": "download"
      }
    ]
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "workerd",
      "sharp"
    ]
  },
  "devDependencies": {}
}
